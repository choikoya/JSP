<meta charset="UTF-8">
<title>구구단</title>
</head>
<body>
<%
String dir= "hor", "ver";

//dir=hor 하면 2~9단 가로로 출력
//dir=ver 하면 세로로 출력



	if("hor".equals(dir)){
		
		for(int i=2; i<10; i++){
			for(int j=1; j<10; j++){
				out.println(i+ " x " + j + " = " + i*j + "<br/>");	
			}
			out.print("<br>");
		}
	}else if("ver".equals(dir)){
			for(j=1; j<10; j++){
				for(i=2; i<10; i++){
					out.print(i+ " x " + j + " = " + i*j);
				}
				out.print("<br>");
			}
			
		}
			
	}
		


%>




</body>
</html>